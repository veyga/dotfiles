{
  "adapters": {
    "python-remote-5680": {
      "port": "5680",
      "launch": {
        "remote": {
          "launchCommmand": [
            "python",
            "-m",
            "debugpy",
            "--listen",
            "0.0.0.0:5680",
            "%CMD%"
          ]
        }
      }
    }
  },
  "configurations": {
    "server": {
      "adapter": "python-remote-5680",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "N",
          "userUnhandled": "N"
        }
      },
      "remote-request": "attach",
      "configuration": {
        "request": "attach",
        "justMyCode": true,
        "pathMappings": [
          {
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}"
          }
        ]
      }
    }
  }
}
