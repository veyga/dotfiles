{
  "adapters": {
    "python-remote-5680": {
      "port": "5680",
      "launch": {
        "remote": {
          "launchCommand": [
            "python",
            "-m",
            "debugpy",
            "--listen",
            "0.0.0.0:5680",
            "%CMD%"
          ]
        }
      }
    },
    "go-remote-delve": {
      "port": "5681",
      "launch": {
        "remote": {
          "launchCommand": [
            "dlv",
            "attach"
          ]
        }
      }
    },
    "go-remote-vs": {
      "port": "5681",
      "connect": {
        "host": "127.0.0.1",
        "port": 5681
      },
      "adapter": "delve",
      "mode": "remote"
    }
  },
  "configurations": {
    "wtf": {
          "name": "Remote Go Debug",
          "type": "go",
          "request": "attach",
          "host": "localhost",
          "port": 5681,
          "trace": true
    },
    "py": {
      "adapter": "python-remote-5680",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "N",
          "userUnhandled": "N"
        }
      },
      "remote-request": "attach",
      "configuration": {
        "request": "attach",
        "justMyCode": true,
        "pathMappings": [
          {
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}"
          }
        ]
      }
    },
    "godelve": {
      "adapter": "go-remote-delve",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "N",
          "userUnhandled": "N"
        }
      },
      "remote-request": "attach",
      "configuration": {
        "request": "attach",
        "mode": "remote",
        "host": "127.0.0.1",
        "port": 5681,
        "justMyCode": true,
        "pathMappings": [
          {
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}"
          }
        ]
      }
    },
    "govs": {
      "adapter": "go-remote-vs",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "N",
          "userUnhandled": "N"
        }
      },
      "remote-request": "attach",
      "configuration": {
        "request": "attach",
        "mode": "remote",
        "host": "127.0.0.1",
        "port": 5681,
        "justMyCode": true,
        "pathMappings": [
          {
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}"
          }
        ]
      }
    }
  }
}
