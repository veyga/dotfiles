{
  "adapters": {
    "python-remote-5680": {
      "port": "5680",
      "launch": {
        "remote": {
          "launchCommmand": [
            "python",
            "-m",
            "debugpy",
            "--listen",
            "0.0.0.0:5680",
            "%CMD%"
          ]
        }
      }
    },
    "go-remote-delve": {
      "port": "5681",
      "launch": {
        "remote": {
          "launchCommand": [
            "dlv",
            "connect",
            "127.0.0.1:5681"
          ]
        }
      }
    },
    "go-remote-vs": {
      "port": "5681",
      "connect": {
        "host": "127.0.0.1",
        "port": 5681
      },
      "adapter": "vscode-go",
      "mode": "remote"
    }
  },
  "configurations": {
    "py": {
      "adapter": "python-remote-5680",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "N",
          "userUnhandled": "N"
        }
      },
      "remote-request": "attach",
      "configuration": {
        "request": "attach",
        "justMyCode": true,
        "pathMappings": [
          {
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}"
          }
        ]
      }
    },
    "go": {
      "adapter": "go-remote-delve",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "N",
          "userUnhandled": "N"
        }
      },
      "remote-request": "attach",
      "configuration": {
        "request": "attach",
        "justMyCode": true,
        "pathMappings": [
          {
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}"
          }
        ]
      }
    }
  }
}
